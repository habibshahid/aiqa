# ==============================================
# TRANSCRIPTION PROVIDER CONFIGURATION
# ==============================================
# Choose your primary transcription provider
# Options: assemblyai, openai, deepgram, soniox
# System will fallback automatically if primary fails
TRANSCRIPTION_PROVIDER=soniox

# ==============================================
# ASSEMBLYAI CONFIGURATION (Primary)
# ==============================================
ASSEMBLYAI_API_KEY=
# Model options: universal (best), nano (fast)
ASSEMBLYAI_MODEL=universal

# ==============================================
# OPENAI WHISPER CONFIGURATION (Fallback)
# ==============================================
OPENAI_API_KEY=
# Model options: whisper-1, gpt-4o-transcribe, gpt-4o-mini-transcribe
OPENAI_WHISPER_MODEL=whisper-1

# ==============================================
# DEEPGRAM CONFIGURATION (Legacy Fallback)
# ==============================================
DEEPGRAM_API_KEY=

# ==============================================
# SONIOX CONFIGURATION (Legacy Fallback)
# ==============================================
SONIOX_API_KEY=your_soniox_api_key_here
# Soniox Model (optional)
SONIOX_MODEL=stt-async-v3


# ==============================================
# COST RATES - ASSEMBLYAI (per minute)
# ==============================================
# Universal model (best accuracy)
COST_STT_ASSEMBLYAI_UNIVERSAL=0.0025
PRICE_STT_ASSEMBLYAI_UNIVERSAL=0.003125

# Nano model (faster, lower accuracy)
COST_STT_ASSEMBLYAI_NANO=0.0015
PRICE_STT_ASSEMBLYAI_NANO=0.001875

# ==============================================
# COST RATES - OPENAI WHISPER (per minute)
# ==============================================
# Whisper-1 model
COST_STT_OPENAI_WHISPER=0.006
PRICE_STT_OPENAI_WHISPER=0.0075

# GPT-4o transcribe model
COST_STT_OPENAI_GPT4O=0.006
PRICE_STT_OPENAI_GPT4O=0.0075

# GPT-4o mini transcribe model
COST_STT_OPENAI_GPT4O_MINI=0.003
PRICE_STT_OPENAI_GPT4O_MINI=0.00375

# ==============================================
# COST RATES - DEEPGRAM (per minute)
# ==============================================
COST_STT_PRERECORDED=0.0052
PRICE_STT_PRERECORDED=0.0065

# ==============================================
# COST RATES - SONIOX (per minute)
# ==============================================
# Soniox pricing: $0.10 per hour of audio
# = $0.10 / 60 minutes = $0.00167 per minute
COST_STT_SONIOX=0.00167
PRICE_STT_SONIOX=0.00208
# Price includes 25% markup: $0.00167 Ã— 1.25 = $0.00208

